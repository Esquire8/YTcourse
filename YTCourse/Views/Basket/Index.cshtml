@model BasketItemViewModel

<div class="container">
    @foreach (var el in Model.basket.listBasketItems){

        <div class="alert alert-warning mt-3">
            <b>Автомобиль:</b> @el.car.name <br/>
            <b>Цена:</b> @el.car.price.ToString("c") <br />
        </div>
    }
    <hr />

    <div class="btn btn-danger" asp-controller="Order">Оплатить</div>
</div>